<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class  name="com.hsqlu.coding.bita.metadata.model.core.ModelElement"
    	table="t_md_element" discriminator-value="ModelElement" >
        <id name="id" type="string">
            <column name="ID" length="32" />
            <generator class="uuid.hex" />
        </id>
        <property name="name" type="string">
            <column name="NAME" length="100">
                <comment>中文名</comment>
            </column>
        </property>
        <property name="version" type="integer">
            <column name="VERSION" />
        </property>
        <property name="type" type="string" not-null="true">
            <column name="TYPE" length="100" />
        </property>
        <property name="memo" type="string">
            <column name="MEMO" length="3000" />
        </property>
        <property name="code" type="string" not-null="true">
            <column name="CODE" length="100">
                <comment>英文名</comment>
            </column>
        </property>
        <property name="ownerId"  type="string" >
            <column name="OWNER_ID" length="32">
            </column>
        </property>
    </class>
</hibernate-mapping>
